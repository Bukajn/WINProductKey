{
  "build": {
    "beforeDevCommand": "bun run dev",
    "beforeBuildCommand": "bun run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "winproductkey",
    "version": "0.2.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "close": true
      }
    },
    "windows": [
      {
        "title": "WINProductKey",
        "center": true,
        "width": 400,
        "height": 100,
        "maximizable": false,
        "minimizable": false,
        "alwaysOnTop": true,
        "resizable": false,
        "decorations": false,
        "transparent": true,
        "closable": false,
        "skipTaskbar": true
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.tauri.dev"
    }
  }
}
